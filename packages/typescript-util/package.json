{
  "private": false,
  "name": "@coolcolduk/typescript-util",
  "version": "6.1.0",
  "description": "utils for typings only",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "main": "./dist/cjs/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build:main": "tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json",
    "build:package": "echo skip",
    "check": "tsc --noEmit",
    "clean:dist": "rm -rf dist && rm -rf tsconfig.tsbuildinfo",
    "clean:module": "rm -rf node_modules",
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix ./src/**/*.{ts,tsx}",
    "pack": "npm pack --pack-destination=../",
    "test": "jest",
    "test:int": "jest --testMatch **/*.spec.ts",
    "test:unit": "jest --testMatch **/*.test.ts"
  },
  "types": "./dist/esm/index.d.ts",
  "devDependencies": {
    "@coolcolduk/eslint-config": "^5.1.0",
    "@coolcolduk/jest-config": "^5.1.0",
    "@coolcolduk/ts-config": "^5.1.0"
  },
  "jest": {
    "preset": "@coolcolduk/jest-config",
    "rootDir": "./src"
  }
}
