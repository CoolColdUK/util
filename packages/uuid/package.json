{
  "private": false,
  "name": "@coolcolduk/uuid",
  "version": "6.2.4",
  "description": "uuid helper function",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.mts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.mjs"
    }
  },
  "main": "./dist/cjs/index.cjs",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build:main": "tsdown",
    "build:package": "echo skip",
    "check": "tsc --noEmit",
    "clean:dist": "rm -rf dist && rm -rf tsconfig.tsbuildinfo",
    "clean:module": "rm -rf node_modules",
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix ./src/**/*.{ts,tsx}",
    "pack": "npm pack --pack-destination=../",
    "test": "jest",
    "test:int": "jest --testMatch **/*.spec.ts",
    "test:unit": "jest --testMatch **/*.test.ts"
  },
  "types": "./dist/esm/index.d.mts",
  "dependencies": {
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@coolcolduk/eslint-config": "^5.1.0",
    "@coolcolduk/jest-config": "^5.1.0",
    "@coolcolduk/ts-config": "^5.1.0",
    "tsdown": "^0.19.0"
  },
  "jest": {
    "preset": "@coolcolduk/jest-config",
    "rootDir": "./src"
  }
}
