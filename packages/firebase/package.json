{
  "private": false,
  "name": "@coolcolduk/firebase",
  "version": "4.1.1",
  "description": "firebase utils",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build:main": "tsc",
    "build:package": "echo skip",
    "check": "tsc --noEmit",
    "clean:dist": "rm -rf dist && rm -rf tsconfig.tsbuildinfo",
    "clean:module": "rm -rf node_modules",
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix ./src/**/*.{ts,tsx}",
    "pack": "npm pack --pack-destination=../",
    "test": "jest",
    "test:int": "jest --testMatch **/*.spec.ts",
    "test:unit": "jest --testMatch **/*.test.ts"
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@coolcolduk/typescript-util": "^4.1.1",
    "@coolcolduk/util": "^4.1.1",
    "express": "^5.0.0",
    "firebase-admin": "^13.4.0"
  },
  "devDependencies": {
    "@coolcolduk/eslint-config": "^4.1.0",
    "@coolcolduk/jest-config": "^4.1.0",
    "@coolcolduk/ts-config": "^4.1.0",
    "@types/express": "^5.0.0"
  },
  "jest": {
    "preset": "@coolcolduk/jest-config",
    "rootDir": "./src"
  }
}
