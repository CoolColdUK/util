{
  "private": false,
  "name": "@coolcolduk/gql-client",
  "version": "5.0.0",
  "description": "gql client related stuff",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build:main": "tsc",
    "build:package": "echo skip",
    "check": "tsc --noEmit",
    "clean:dist": "rm -rf dist && rm -rf tsconfig.tsbuildinfo",
    "clean:module": "rm -rf node_modules",
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix ./src/**/*.{ts,tsx}",
    "pack": "npm pack --pack-destination=../",
    "test": "jest",
    "test:int": "jest --testMatch **/*.spec.ts",
    "test:unit": "jest --testMatch **/*.test.ts"
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@apollo/client": "^4.0.0",
    "@coolcolduk/enum": "^5.0.0",
    "@coolcolduk/util": "^5.0.0"
  },
  "devDependencies": {
    "@coolcolduk/eslint-config": "^5.0.0",
    "@coolcolduk/jest-config": "^5.0.0",
    "@coolcolduk/ts-config": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0"
  },
  "jest": {
    "preset": "@coolcolduk/jest-config",
    "rootDir": "./src"
  }
}
