{
  "private": false,
  "name": "@coolcolduk/gql",
  "version": "7.1.0",
  "description": "various gql server related stuff",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.mts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.mjs"
    }
  },
  "main": "./dist/cjs/index.cjs",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build:main": "tsdown",
    "build:package": "echo skip",
    "check": "tsc --noEmit",
    "clean:dist": "rm -rf dist && rm -rf tsconfig.tsbuildinfo",
    "clean:module": "rm -rf node_modules",
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix ./src/**/*.{ts,tsx}",
    "pack": "npm pack --pack-destination=../",
    "test": "jest",
    "test:int": "jest --testMatch **/*.spec.ts",
    "test:unit": "jest --testMatch **/*.test.ts"
  },
  "types": "./dist/esm/index.d.mts",
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/express5": "^1.0.0",
    "@coolcolduk/enum": "^7.1.0",
    "cors": "^2.8.5",
    "express": "^5.0.0",
    "graphql": "^16.11.0",
    "type-graphql": "^2.0.0-rc.2"
  },
  "devDependencies": {
    "@coolcolduk/eslint-config": "^5.1.0",
    "@coolcolduk/jest-config": "^5.1.0",
    "@coolcolduk/ts-config": "^5.1.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "tsdown": "^0.20.1"
  },
  "jest": {
    "preset": "@coolcolduk/jest-config",
    "rootDir": "./src"
  }
}
